generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../../../data/metastable.db"
}

model Project {
  id         Int      @id @default(autoincrement())
  name       String
  settings   String?
  lastOutput String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model Model {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  imageUrl    String?
  type        String
  filename    String
  source      String?
  sourceId    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@unique([type, filename])
}
