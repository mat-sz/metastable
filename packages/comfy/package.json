{
  "name": "@metastable/comfy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup:amd:rocm5.6": "pip3 install --target=./python/.pip --extra-index-url https://download.pytorch.org/whl/rocm5.6 -r ./python/requirements.txt",
    "setup:amd:rocm5.7": "pip3 install --target=./python/.pip --pre --extra-index-url https://download.pytorch.org/whl/nightly/rocm5.7 -r ./python/requirements.txt",
    "setup:nvidia": "pip3 install --target=./python/.pip --extra-index-url https://download.pytorch.org/whl/cu121 -r ./python/requirements.txt",
    "setup:other": "pip3 install --target=./python/.pip -r ./python/requirements.txt",
    "setup:amd": "yarn setup:amd:rocm5.6",
    "build": "tsc"
  },
  "devDependencies": {
    "@metastable/types": "*",
    "@types/node": "^20.10.3",
    "@types/semver": "^7.5.6",
    "@types/which": "^3.0.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "semver": "^7.5.4",
    "which": "^4.0.0"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  }
}
