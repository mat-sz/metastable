{
  "name": "@metastable/metastable",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup:amd:rocm": "pip3 install --extra-index-url https://download.pytorch.org/whl/rocm6.1 -r ./python/requirements.txt",
    "setup:nvidia": "pip3 install --extra-index-url https://download.pytorch.org/whl/cu124 -r ./python/requirements.txt",
    "setup:other": "pip3 install -r ./python/requirements.txt",
    "setup:amd": "yarn setup:amd:rocm",
    "rebuild": "electron-rebuild",
    "build": "tsc",
    "lint": "run -T eslint src",
    "test": "run -T vitest"
  },
  "peerDependencies": {
    "electron": "*",
    "electron-updater": "*"
  },
  "devDependencies": {
    "@metastable/types": "workspace:^",
    "@types/event-stream": "^4.0.5",
    "@types/node": "^22.10.5",
    "@types/semver": "^7.5.8",
    "@types/tar-stream": "^3.1.3",
    "@types/which": "^3.0.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@metastable/common": "workspace:^",
    "@metastable/model-info": "workspace:^",
    "@trpc/server": "^11.0.0-rc.601",
    "chokidar": "^3.5.3",
    "event-stream": "^4.0.1",
    "js-base64": "^3.7.7",
    "meta-png": "^1.0.6",
    "nanoid": "^5.0.4",
    "radash": "^12.1.0",
    "semver": "^7.6.2",
    "sharp": "^0.33.2",
    "tar-stream": "^3.1.7",
    "which": "^4.0.0",
    "zod": "^3.22.4"
  },
  "imports": {
    "#metastable": "./lib/metastable.js",
    "#helpers/*": "./lib/helpers/*"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20"
  }
}
