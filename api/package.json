{
  "name": "api",
  "type": "module",
  "scripts": {
    "setup:amd:rocm5.6": "pip3 install --target=./python/.pip torch torchvision torchaudio --index-url https://download.pytorch.org/whl/rocm5.6",
    "setup:amd:rocm5.7": "pip3 install --target=./python/.pip --pre torch torchvision torchaudio --index-url https://download.pytorch.org/whl/nightly/rocm5.7",
    "setup:nvidia": "pip3 install --target=./python/.pip torch torchvision torchaudio --extra-index-url https://download.pytorch.org/whl/cu121",
    "setup:other": "pip3 install torch torchvision torchaudio",
    "setup:amd": "yarn setup:amd:rocm5.6",
    "setup:pip": "pip3 install --target=./python/.pip -r ./python/requirements.txt",
    "start": "tsx src/index.ts",
    "start:prod": "node build/index.js",
    "build": "tsc"
  },
  "devDependencies": {
    "@fastify/http-proxy": "^9.3.0",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^20.10.3",
    "@types/ws": "^8.5.10",
    "prisma": "^5.6.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^8.2.0",
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.2",
    "dotenv-flow": "^4.0.1",
    "fastify": "^4.24.3",
    "http-errors": "^2.0.0",
    "joi": "^17.11.0",
    "json-schema-to-ts": "^2.9.2",
    "nanoid": "^5.0.4",
    "ws": "^8.14.2"
  }
}
